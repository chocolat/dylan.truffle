(detector
    (extension lid))

(in source.dylan
    (shell-variables
        (TM_COMMENT_START [// ])
        (TM_COMMENT_START_2 /*)
        (TM_COMMENT_END_2 */)
        (TM_COMMENT_DISABLE_INDENT_2 yes))
    (comment.line [// ])
    (comment.block /* */))

(in [comment.block.dylan, comment.line.double-slash.dylan]
    (spellchecking))

(in [source.dylan entity.name.other]
    (symbol-list))